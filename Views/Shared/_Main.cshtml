    <script type="text/javascript">
         $("#logoff").click(function (e) {
            e.preventDefault();

            $.ajax({
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                type: 'POST',
                url: "/Login/logout",
                success: function (result) {
                    if (result.status == 200){
                        window.location.href = '/Login/';
                    }
                }
            });
        });

